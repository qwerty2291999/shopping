{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "import/prefer-default-export": "off",
    "import/extensions": ["off", "never", { "js": "always", "json": "never" }],
    "consistent-return": ["error", { "treatUndefinedAsUnspecified": true }]
  }
}
